<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
 						xmlns:geo="http://a9.com/-/opensearch/extensions/geo/1.0/">
   <ShortName>CatalogConnector</ShortName>
   <Description>Search in many catalogues</Description>
   <Tags>Catalog CSW CatalogConnector</Tags>
   <Contact>idec@icc.cat</Contact>
   <Url type="text/html"
        template="http://localhost:8080/catalogConnector/Connector?Request=GetRecords&amp;Project=catalogues&amp;outputFormat=HTML&amp;catalogues=FAO&amp;any={searchTerms}&amp;maxRecords={count?}&amp;startPosition={startIndex?}&amp;bbox={geo:box?}"/>
 
   <Url type="application/vnd.google-earth.kml+xml"
        template="http://localhost:8080/catalogConnector/Connector?Request=GetRecords&amp;Project=catalogues&amp;outputFormat=KML&amp;catalogues=FAO&amp;any={searchTerms}&amp;maxRecords={count?}&amp;startPosition={startIndex?}&amp;bbox={geo:box?}"/>

